<div id="events">

  <div class="tabs text-center">
    <%= link_to "Profile", user_path(current_user), class: "tab" %>
    <%= link_to "My swings", "#", class: "tab active" %>
    <%= link_to "My participations", user_participations_path(current_user), class: "tab" %>
  </div>


  <% @my_events.each do |event| %>
    <div class="event">
      <div class="details">
        <%= event.start_at %>
      </div>
      <div class="title">
        <%= event.title %>
      </div>
      <div class="description">
        <%= event.description %>
      </div>
    </div>
  <% end %>
  <div class="swing">
    <%= link_to "Create a Swing", new_event_path, class: "btn btn-primary cta" %>
  </div>
</div>



